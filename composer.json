{
  "name": "fusio/sdk",
  "description": "SDK to talk to the Fusio REST API",
  "homepage": "https://www.fusio-project.org",
  "license": "MIT",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "http://christoph.kappestein.com"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^6.3",
    "psx/api": "^3.0",
    "psx/schema": "^3.0",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "vimeo/psalm": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Fusio\\Sdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fusio\\Sdk\\Tests\\": "tests/"
    }
  }
